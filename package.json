{
    "name": "harply-bot-web-app",
    "version": "0.0.1",
    "description": "Simple message style based chat bot web app",
    "repository": "https://github.com/OldFresco/harply-ui",
    "scripts": {
        "lite-server": "lite-server",
        "lint-check": "tslint",
        "lint": "npm run lint-check \"src/**/*.ts\"",
        "test": "npm run build && karma start karma.conf.js",
        "install-typings": "typings install",
        "post-install": "npm run install-typings",
        "build": "webpack --inline --colors --progress --display-error-details --display-cached",
        "watch": "npm run build -- --watch",
        "dev-server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 8000  --content-base src",
        "start": "npm run dev-server"
    },
    "dependencies": {
        "@angular/common": "^2.0.0-rc.5",
        "@angular/compiler": "^2.0.0-rc.5",
        "@angular/core": "^2.0.0-rc.5",
        "@angular/forms": "^0.3.0",
        "@angular/platform-browser": "^2.0.0-rc.5",
        "@angular/platform-browser-dynamic": "^2.0.0-rc.5",
        "@angular/http": "2.0.0-rc.5",
        "@angular/router": "^3.0.0-rc.1",
        "reflect-metadata": "^0.1.3",
        "rxjs": "5.0.0-beta.6",
        "zone.js": "0.6.12",
        "core-js": "^2.2.0",
        "ie-shim": "^0.1.0"
    },
    "devDependencies": {
        "awesome-typescript-loader": "^1.1.1",
        "css-loader": "^0.23.1",
        "copy-webpack-plugin": "^3.0.1",
        "html-webpack-plugin": "^2.21.0",
        "raw-loader": "^0.5.1",
        "to-string-loader": "^1.1.4",
        "webpack": "^1.12.9",
        "webpack-dev-server": "^1.14.0",
        "webpack-merge": "^0.8.4",
        "tslint": "^3.14.0",
        "typings": "^1.0.4",
        "aliv": "^1.1.5",
        "jasmine": "^2.3.2",
        "karma": "^0.13.15",
        "karma-chrome-launcher": "^0.2.1",
        "karma-jasmine": "^0.3.6",
        "typescript": "^2.0.0"
    }
}